{
  "name": "skypro-music",
  "homepage": "https://akhorolich.github.io/skypro-music",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predeploy": "npm run build && touch out/.nojekyll",
    "deploy": "gh-pages --dotfiles --dist out",
    "build": "next build",
    "dev": "next dev",
    "dev-inspect": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "lint": "eslint",
    "preview:gh-pages": "rm -r ./out && npm run build && mkdir out/skypro-music && find out -mindepth 1 -maxdepth 1 ! -name skypro-music -exec mv {} out/skypro-music/ \\; && node gh-pages-script.js && npx serve@latest out"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@next/eslint-plugin-next": "^15.5.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "steiger": "^0.5.11",
    "typescript": "^5"
  }
}
